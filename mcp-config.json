{
  "mcpServers": {
    "filesystem": {
      "command": "bunx",
      "args": ["mcp-server-filesystem", "assets", "downloads"]
    },
    "uplink": {
      "command": "bun",
      "args": [
        "run",
        "servers/cdn/uplink-server.ts",
        "optional-cdn-folder-a",
        "optional-cdn-folder-b"
      ]
    }
  },

  "localAgent": {
    "host": "http://localhost:11434",
    "modelId": "gpt-oss:20b"
  },
  "remoteAgent": {
    "host": "https://console.groq.com",
    "modelId": "openai/gpt-oss-20b"
  },
  "agentProvider": "localAgent",
  "isChatEnabled": true
}
